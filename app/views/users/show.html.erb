<h2 style="text-align: center">マイプロフィール</h2>
<div class="container user-profile">
    <h2><%= @user.nickname %></h1>
    <p><%= link_to image_tag(@user.avatar.url , alt: @user.nickname), user_path(@user) %></p>
</div><br/>
<div class="container user-profile-projects">
    <h4>自分のプロジェクト</h4>
    <%= render 'projects/list' , projects: @projects %>
    <h4>参加しているプロジェクト</h4>
    <%= render 'projects/list', projects: @joinProjects %>
    
</div>
